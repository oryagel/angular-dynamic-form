{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "dynamic-form": {
      "projectType": "library",
      "root": "projects/dynamic-form",
      "sourceRoot": "projects/dynamic-form/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/dynamic-form/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/dynamic-form/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/dynamic-form/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/dynamic-form/src/test.ts",
            "tsConfig": "projects/dynamic-form/tsconfig.spec.json",
            "karmaConfig": "karma-configs/karma.conf.base.js"
          },
          "configurations": {
            "single-run": {
              "karmaConfig": "karma-configs/karma.conf.ci.js"
            }
          }
        }
      }
    },
    "dynamic-form-wizard": {
      "projectType": "library",
      "root": "projects/dynamic-form-wizard",
      "sourceRoot": "projects/dynamic-form-wizard/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/dynamic-form-wizard/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/dynamic-form-wizard/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/dynamic-form-wizard/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/dynamic-form-wizard/src/test.ts",
            "tsConfig": "projects/dynamic-form-wizard/tsconfig.spec.json",
            "karmaConfig": "projects/dynamic-form-wizard/karma.conf.js"
          }
        }
      }
    }
  }
}
